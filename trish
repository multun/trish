#!/usr/bin/env python3

import sys

from libtrish import compare
from libtrish.conf import DefaultTrishConf

if __name__ == '__main__':
    if len(sys.argv) < 3:
        print(f"Usage: {sys.argv[0]} fileA fileB")
        exit(1)
    conf = DefaultTrishConf()
    compare(conf, sys.argv[1], sys.argv[2])
